<!-- searchResults.ejs -->

<h2>Search Results</h2>

<% if (results.length > 0) { %>
    <ul>
        <% results.forEach(item => { %>
            <li><%= item.sport_name %> - <%= item.item_name %> - <%= item.quantity %></li>
        <% }); %>
    </ul>
<% } else { %>
    <p>No results found.</p>
<% } %>
